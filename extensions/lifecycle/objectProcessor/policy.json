{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "LifecycleExpirationObjectProcessor",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:DeleteObject",
                "s3:DeleteObjectVersion",
                "s3:AbortMultipartUpload",
                "s3:ReplicateObject"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}
